<div class="span12">
	<table border="1"  class="table table-hover listings">
		<thead
		<tr>
		<th>id</th>
		<th>User Name</th>
		<th>First</th>
		<th>Last</th>
		<th>Email</th>
		<th>Sign In Count</th>
		<th>Last Seen</th>
		<th>Admin</th>
		</tr>
	</thead>
		<tbody>
		  <% @users.last(3).each do |user| %>
		  	<tr class="admin-lists" onclick="document.location = '/admin/user/<%= user.id %>';" >
		      <%= render 'user', user: user %>
				 </tr>
		  <% end %> 
		<tbody>
	</table>
</div>
